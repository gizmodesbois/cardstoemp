<!doctype html>
<html {% if section %} data-current="{{ section }}"{% endif %} lang="en">
    <head>
        <meta name="robots" content="noindex, nofollow">
    </head>
	<body>
		<header>
			<em>
				<a href="{{ '/' | url }}">{{ metadata.title }}</a>
			</em>
		</header>
		<main>
			<ul>
            {%- for post in collections.post -%}
              <li>{{ post.data.title }}</li>
            {%- endfor -%}
            </ul>
		</main>
	</body>
</html>
